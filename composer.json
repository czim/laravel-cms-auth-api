{
    "name": "czim/laravel-cms-auth-api",
    "description": "Laravel CMS: Authentication Component API Extension",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com"
        }
    ],
    "keywords": [ "cms", "laravel", "authentication", "api", "oauth2" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=5.6.4",
        "czim/oauth2-server-laravel": "~5.4.0|~5.5.0"
    },
    "require-dev": {
        "czim/laravel-cms-core": "~1.4",
        "czim/laravel-cms-auth": "1.*",
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.4",
        "orchestra/database": "~3.4",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsAuthApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsAuthApi\\Test\\": "tests",
            "Czim\\CmsCore\\Test\\": "vendor/czim/laravel-cms-core/tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
